{
    "extends": "./.svelte-kit/tsconfig.json",
    "compilerOptions": {
        "paths": {
            "src/*": ["src/*"],
            "styles/*": ["src/styles/*"],
            "interfaces/*": ["src/interfaces/*"],
            "$lib":["src/lib"],
            "$lib/*":["src/lib/*"]
        },
        "pretty": true
    }
}